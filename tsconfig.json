{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "allowJs": true,
    "jsxImportSource": "react",
    "paths": {
      "@/*": ["src/*"],
      "components/*": ["./node_modules/react-dev/src/components/*"]
    }
  },
  "include": ["src", "./node_modules/react-dev/src"]
}
